{
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
        "html"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style": 0,
        "import/extensions": 0
    },
    "globals": {
        "Vue": false
    }
}